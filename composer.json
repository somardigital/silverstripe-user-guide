{
    "name": "somardigital/silverstripe-user-guide",
	"description": "A silverstripe module providing embedded CMS User Guide Documentation",
	"type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "CMS",
        "user guide"
    ],
    "license": "BSD-3-Clause",
    "require": {
        "php": "^8.3",
        "silverstripe/framework": "^6.0",
        "silverstripe/admin": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Somar\\UserGuide\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
